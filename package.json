  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/pdf-parse": "^1.1.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "tsx": "^4.19.4",
    "vercel": "^42.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
