{
  "name": "portfolio-kpi-copilot",
  "version": "1.0.5",
  "description": "Enterprise Portfolio KPI Copilot - Advanced AI-powered KPI analysis and management",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf node_modules/.prisma && (prisma generate || echo 'Prisma generation failed, using fallback') && next build",
    "postinstall": "prisma generate || echo 'Prisma generation failed, continuing...'",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/seed-data.ts",
    "setup": "tsx scripts/setup-production.ts",
    "setup:ai": "tsx scripts/setup-ai-integration.ts",
    "setup:dual": "tsx scripts/setup-dual-integration.ts",
    "setup:env": "./scripts/setup-env.sh",
    "setup:ollama": "./scripts/setup-ollama-production.sh",
    "deploy:rapid": "./scripts/rapid-deploy.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:hybrid": "./scripts/test-hybrid-functionality.sh",
    "test:auth": "./scripts/test-authentication.sh",
    "vercel:build": "npm install --legacy-peer-deps && rm -rf node_modules/.prisma && (prisma generate || echo 'Prisma generation failed, using fallback') && next build",
    "vercel:dev": "npm run db:generate && next dev",
    "deploy:check": "npm run lint && npm run type-check && npm run build",
    "business:metrics": "node -e \"console.log('Business metrics tracking enabled')\"",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "status:comprehensive": "curl -f http://localhost:3000/api/system/comprehensive-status",
    "ollama:start": "ollama serve",
    "ollama:test": "curl -s http://localhost:11434/api/tags",
    "backend:verify": "npm run status:comprehensive && npm run health:check"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@hookform/resolvers": "^3.3.2",
    "@langchain/community": "^0.2.0",
    "@langchain/core": "^0.2.0",
    "@langchain/openai": "^0.1.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@pinecone-database/pinecone": "^2.2.0",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.9",
    "@tanstack/react-query": "^5.17.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.16",
    "langchain": "^0.2.0",
    "lucide-react": "^0.303.0",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.6",
    "openai": "^4.104.0",
    "pdf-parse": "^1.1.1",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "redis": "^5.5.6",
    "sonner": "^2.0.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.0",
